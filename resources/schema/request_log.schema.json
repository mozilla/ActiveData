{
	"settings": {
		"index.store.throttle.type": "merge",
		"index.number_of_replicas": 0,
		"index.store.throttle.max_bytes_per_sec": "2mb",
		"index.number_of_shards": 3
	},
	"mappings": {
		"request_log": {
			"dynamic_templates": [
				{
					"default_strings": {
						"mapping": {
							"index": "not_analyzed",
							"type": "string"
						},
						"match_mapping_type": "string",
						"match": "*"
					}
				}
			],
			"properties": {
				"path": {
					"index": "not_analyzed",
					"type": "string"
				},
				"remote_addr": {
					"index": "not_analyzed",
					"type": "string"
				},
				"http_accept_encoding": {
					"index": "not_analyzed",
					"type": "string"
				},
				"query": {
					"type": "object",
					"enabled": false
				},
				"query_text": {
					"type": "string",
					"store": "yes"
				},
				"data": {
					"index": "not_analyzed",
					"type": "string"
				},
				"error": {
					"type": "object",
					"enabled": false
				},
				"from": {
					"index": "not_analyzed",
					"type": "string"
				},
				"content_length": {
					"index": "not_analyzed",
					"type": "string"
				},
				"http_user_agent": {
					"index": "not_analyzed",
					"type": "string"
				},
				"timestamp":{
					"type": "double"
				}

			}

		}
	}
}

