{
	"flask": {
		"host": "0.0.0.0",
		"port": 5000,
		"debug": false,
		"threaded": true,
		"processes": 1,
		"ssl_context":"adhoc",
		"allow_exit": true
//		"ssl_context": {
//			"privatekey_file": "~/keys/activedata.allizom.org/activedata.allizom.org.key",
//			"certificate_file": "~/keys/activedata.allizom.org/activedata_allizom_org.crt",
//			"certificate_chain_file": "~/keys/activedata.allizom.org/DigiCertCA.crt"
//		}
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
		"pyLibrary.env.big_data.MAX_STRING_SIZE": 100000000,
		"jx_elasticsearch.meta.ENABLE_META_SCAN": true,
		"jx_base.expressions.ALLOW_SCRIPTING": true
	},
	"request_logs": {
		"host": "http://localhost",
		"port": 9200,
		"index": "active_data_requests",
		"type": "request_log",
		"schema": {
			"$ref": "//../../resources/schema/request_log.schema.json"
		}
	},
	"saved_queries": {
		"host": "http://localhost",
		"port": 9200,
		"index": "saved_queries",
		"type": "query",
		"debug": true
	},
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9200,
		"index": "testdata",
		"type": "test_result",
		"debug": true
	},
	"debug": {
		"trace": true,
		"cprofile": {
			"enabled": false,
			"filename": "./results/profile.tab"
		},
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "email",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][DEV]Problem with ActiveData Server",
				"$ref": "file://~/private.json#email"
			},
			{
				"log_type": "elasticsearch",
				"host": "http://localhost",
				"index": "debug_active_data",
				"type": "active_data"
			}
		]
	}

}
