{
	"use":"elasticsearch",

	"fastTesting": false,  //IMPORTANT VARIABLE!  true WILL RUN TESTS FASTER, false INCLUDES NETWORK RESPONSE TESTS
	"testing":{
		"query": "http://127.0.0.1:5000/query",
		"sql": "http://127.0.0.1:5000/sql"
	},
	"backend_es": {
		"index": "testdata",
		"type": "test_result",
		"host": "http://localhost",
		"port": 9200,
		"schema": {
			"settings": {"index": {"number_of_shards": 1}},
			"$ref": "//.../resources/schema/basic_schema.json.template?type=test_result"
		},
		"debug": true
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "ActiveDataTests"
		},
		"jx_elasticsearch.es52.bulk_aggs.S3_CONFIG": true
	},
	"flask": {
		"host": "0.0.0.0",
		"port": 5000,
		"debug": false,
		"threaded": true,
		"processes": 1
	},
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9200,
		"index": "testdata",
		"type": "test_result",
		"debug": true
	},
	"debug": {
		"trace": true
	}
}
